<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Amiens Sport Tennis de Table{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" media="screen" charset="utf-8" href="{{ asset('bundles/fsbasttfront/css/bootstrap/bootstrap-responsive.min.css') }}" />
            <link rel="stylesheet" type="text/css" media="screen" charset="utf-8" href="{{ asset('bundles/fsbasttfront/css/bootstrap/bootstrap-datepicker.css') }}" />
        {% endblock %}
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div class="header">
            {% block header %}
                 <h1><a href="{{ path('FSBASTTFrontBundle_homepage') }}" title="Revenir Ã  l'accueil">Amiens Sport Tennis de Table</a></h1>
            {% endblock %}
        </div>
        
        <div class="{% block class_nav %}{% endblock %}">
            {% block nav %}
                 
            {% endblock %}
        </div>
        
        {% block body %}
            <div id="main" class="{% block class_main %}{% endblock %}">
                {% block main %}
                    
                {% endblock %}
            </div>
        {% endblock %}
        
        <div class="footer">
            {% block footer %}
                 
            {% endblock %}
        </div>
        
        {% block javascripts %}
            <script type="text/javascript" src="{{ asset('bundles/fsbasttfront/js/jquery-1.7.2.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/fsbasttfront/js/bootstrap.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/fsbasttfront/js/bootstrap-datepicker.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/fsbasttfront/js/bootbox.min.js') }}"></script>
        {% endblock %}
        
        {# {% include "GoogleBundle:Analytics:async.html.twig" %} #}
    </body>
</html>
